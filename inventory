# Exemplo de invent√°rio com diferentes tipos de equipamentos
# Substitua os IPs e nomes de hosts pelos valores corretos para o seu ambiente

[dell_os10]
dellos10-switch1 ansible_host=192.168.10.1 ansible_network_os=dellemc.os10.os10

[dell_os6]
dellos6-switch1 ansible_host=192.168.6.1 ansible_network_os=dellemc.os6.os6

[aruba_aoscx]
# Para switches Aruba com OS-CX
aruba-switch1 ansible_host=192.168.20.1 ansible_network_os=arubanetworks.aoscx.aoscx

[hpe_procurve]
# Para switches HPE ProCurve / ArubaOS-Switch
procurve-switch1 ansible_host=192.168.30.1 ansible_network_os=hpe.procurve.procurve

[fortigates]
fortigate1 ansible_host=192.168.1.254 ansible_network_os=fortinet.fortios.fortios

# --- Grupos para o Playbook ---
# O playbook executa as roles nos grupos 'switches' e 'firewalls'
[switches:children]
dell_os10
dell_os6
aruba_aoscx
hpe_procurve

[firewalls:children]
fortigates